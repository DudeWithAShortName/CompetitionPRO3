@model Competition_PRO.Models.ListovkiAllInputModel



<form method="post">
    <h1>Listovki</h1>
   <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Име</th>
                <th scope="col">Училище</th>
                <th scope="col">Точки</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.ListovkiInputModels)
            {
                <tr>
                    <td>@item.ParticipantNumber</td>
                    <td>@item.Name</td>
                    <td>@item.SchoolName</td>
                    <td><input asp-for="@item.Points" /></td>
                </tr>
            }
        </tbody>
    </table>
    <input type="submit" class="btn btn-primary" />
</form>
